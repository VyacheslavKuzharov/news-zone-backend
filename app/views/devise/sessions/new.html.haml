= form_for(resource, as: resource_name, url: session_path(resource_name), html: {class: 'form-signin'}) do |f|
  %h2.form-signin-heading Please sign in
  = f.label :email, class: 'sr-only'
  = f.email_field :email, placeholder: 'Email address', autofocus: true, id: 'inputEmail', class: 'form-control'
  = f.label :password, class: 'sr-only'
  = f.password_field :password, autocomplete: "off", placeholder: 'Password', id: 'inputPassword', class: 'form-control'

  = f.submit 'Sign in', class: 'btn btn-lg btn-primary btn-block'
